library(dplyr)
# 创建示例数据
set.seed(123)
data <- data.frame(
Group = rep(c("Group A", "Group B"), each = 4),      # 两组：A和B
Category = rep(c("Cat1", "Cat2", "Cat3", "Cat4"), 2), # 每组4个类别
Value = c(rnorm(4, mean = 10, sd = 2),              # Group A的值
rnorm(4, mean = 8, sd = 1.5))              # Group B的值
)
# 绘制分组柱状图
ggplot(data, aes(x = Group, y = Value, fill = Category)) +
geom_col(position = position_dodge(width = 0.8),      # 分组柱状图，设置间距
width = 0.7) +                               # 柱状图宽度
geom_text(aes(label = round(Value, 1)),               # 添加数值标签
position = position_dodge(width = 0.8),     # 标签与柱子对齐
vjust = -0.5, size = 3.5) +                 # 标签位置和大小
scale_fill_brewer(palette = "Set2") +                 # 使用ColorBrewer配色
labs(title = "分组柱状图示例",                        # 标题和轴标签
x = "组别",
y = "数值",
fill = "类别") +
theme_minimal() +                                     # 简洁主题
theme(
plot.title = element_text(hjust = 0.5, size = 14, face = "bold"), # 标题居中
axis.text.x = element_text(size = 10),
legend.position = "top"                             # 图例在顶部
)
